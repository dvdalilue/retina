func my_forward()
begin
    forward(1);
end;

func max(number x, number y) -> number
begin
    if x < y then
        return y;
    else
        return x;
    end;
end;

program
    my_forward();
    with do end; # Dummy block
    2; # This 2 does nothing
    with
        number x = 37;
    do
        writeln "Un numero especial: ", max(9,x);
    end;
end;